{
    "manifest_version": 2,
    "name": "Coxmotron",
    "description": "Tieba Spammer by SorenEricMent",
    "version": "0.0.1",
    "permissions": [
        "tabs",
        "https://api.winsloweric.cn/",
        "https://img.projectstardust.org/",
        "https://tieba.baidu.com/",
        "contextMenus",
		"tabs",
		"notifications",
		"webRequest",
		"webRequestBlocking",
		"storage"
    ],
    "icons":
	{
		"16": "images/icon.png",
		"48": "images/icon.png",
		"128": "images/icon.png"
	},
    "content_scripts": 
	[
		{
			//"matches": ["http://*/*", "https://*/*"],
			// "<all_urls>" 表示匹配所有地址
			"matches": ["<all_urls>"],
			// 多个JS按顺序注入
			"js": ["scripts/jquery.js","scripts/kernal.js"],
			// JS的注入可以随便一点，但是CSS的注意就要千万小心了，因为一不小心就可能影响全局样式
			"css": ["stylesheets/console.css"],
			// 代码注入的时间，可选值： "document_start", "document_end", or "document_idle"，最后一个表示页面空闲时，默认document_idle
			"run_at": "document_start"
		}
	],
    "browser_action": 
	{
		"default_icon": "images/icon.png",
		"default_title": "Tieba Spammer by SorenEricMent",
		"default_popup": "index.html"
	},

	"homepage_url": "https://www.winsloweric.cn"
	
}